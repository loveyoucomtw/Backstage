        console.log(e.data);\n        if (e.data === \"guest\") {\n          this.isLogin = false;\n        } else {\n          this.isLogin = true;\n          this.account = e.data;\n        }\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  },\n};\n</script>\n\n<style>\nbody {\n  padding: 0;\n  margin: 0;\n}\n</style>\n"]}]}